{
  "name": "antd-starterkit-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env mode=local vue-cli-service serve --open --mode local",
    "build:dev": "cross-env mode=dev vue-cli-service build",
    "build:sit": "cross-env mode=sit vue-cli-service build",
    "build:prod": "cross-env mode=prod vue-cli-service build"
  },
  "dependencies": {
    "@ant-design/colors": "^3.2.2",
    "@antv/data-set": "^0.10.2",
    "@antv/g2": "^3.5.9",
    "@tinymce/tinymce-vue": "^2.1.0",
    "ant-design-vue": "^1.4.3",
    "area-data": "^5.0.6",
    "axios": "^0.21.1",
    "codemirror": "^5.48.2",
    "compression-webpack-plugin": "^3.0.0",
    "core-js": "^2.6.5",
    "cross-env": "^5.2.0",
    "crypto-js": "^3.1.9-1",
    "echarts": "^4.4.0",
    "enquire.js": "^2.1.6",
    "file-saver": "^2.0.2",
    "increase-memory-limit": "^1.0.7",
    "js-cookie": "^2.2.0",
    "lodash.filter": "^4.6.0",
    "lodash.get": "^4.4.2",
    "lodash.isfunction": "^3.0.9",
    "lodash.isnan": "^3.0.2",
    "lodash.kebabcase": "^4.1.1",
    "lodash.keys": "^4.2.0",
    "lodash.pick": "^4.4.0",
    "lodash.replace": "^4.1.4",
    "lodash.tonumber": "^4.0.3",
    "lodash.tostring": "^4.1.4",
    "lodash.values": "^4.3.0",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "nprogress": "^0.2.0",
    "os": "^0.1.1",
    "qs": "^6.9.0",
    "script-loader": "^0.7.2",
    "tinymce": "^5.0.13",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vconsole": "^3.3.4",
    "vue": "^2.6.10",
    "vue-area-linkage": "^5.1.0",
    "vue-clipboard2": "^0.3.0",
    "vue-cropper": "0.4.4",
    "vue-echarts": "^4.0.4",
    "vue-ls": "^3.2.0",
    "vue-plyr": "^5.1.3",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.1.3",
    "vue-svg-component-runtime": "^1.0.1",
    "vuedraggable": "^2.23.0",
    "vuex": "^3.1.1",
    "websocket": "^1.0.30"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.2.5",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.12.0",
    "eslint": "^5.16.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-vue": "^5.1.0",
    "less": "^3.10.0",
    "less-loader": "^5.0.0",
    "sass": "^1.22.9",
    "sass-loader": "^7.2.0",
    "vue-svg-icon-loader": "^2.1.1",
    "vue-template-compiler": "^2.6.10",
    "webpack-cli": "^3.3.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "generator-star-spacing": "off",
      "no-mixed-operators": 0,
      "vue/max-attributes-per-line": [
        2,
        {
          "singleline": 5,
          "multiline": {
            "max": 1,
            "allowFirstLine": false
          }
        }
      ],
      "vue/attribute-hyphenation": 0,
      "vue/html-self-closing": 0,
      "vue/component-name-in-template-casing": 0,
      "vue/html-closing-bracket-spacing": 0,
      "vue/singleline-html-element-content-newline": 0,
      "vue/no-unused-components": 0,
      "vue/multiline-html-element-content-newline": 0,
      "vue/no-use-v-if-with-v-for": 0,
      "vue/html-closing-bracket-newline": 0,
      "vue/no-parsing-error": 0,
      "no-console": 0,
      "spaced-comment": [
        "error",
        "always",
        {
          "line": {
            "markers": [
              "/"
            ],
            "exceptions": [
              "-",
              "+"
            ]
          },
          "block": {
            "markers": [
              "!"
            ],
            "exceptions": [
              "*"
            ],
            "balanced": true
          }
        }
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
